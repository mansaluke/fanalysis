    "        \n",
    "        self.out_tree(self.xe, self.r, self.len)  \n",
    "    \n",
    "    \n",
    "    def out_tree(self, xe=3, r = 0.25, len = 0.75):\n",
    "        \"\"\"\n",
    "        xe = num of nodes, r= radius, len = length of lines\n",
    "        \"\"\"    \n",
    "        \n",
    "        b=[0]\n",
    "        self.ax.add_patch(Circle((r, 0), r, facecolor='none', edgecolor = 'black')) \n",
    "        \n",

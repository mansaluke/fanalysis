    "    \n",
    "    def __init__(self, xe, r, len):\n",
    "        self.xe, self.r, self.len = xe, r, len\n",
    "        self.xlim = [0, self.xe + 1]\n",
    "        self.ylim = [-r-len, r+len]\n",
    "        \n",
    "        fig, self.ax = plt.subplots(frameon=False)\n",
    "    \n",
